{
  "expo": {
    "name": "ByteWise Nutritionist",
    "slug": "bytewise-nutritionist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icon-512.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./icon-512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bytewise.nutritionist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./icon-512.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bytewise.nutritionist"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    }
  }
}